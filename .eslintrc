{
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "react",
    "prettier"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          "jsx"
        ]
      }
    ],
    "no-shadow":"warn",
    "prettier/prettier": "error",
    "max-len": [
      "error",
      80
    ]
  },
  "env": {
    "browser": true,
    "es6": true
  }
}